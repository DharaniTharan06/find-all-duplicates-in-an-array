#include <stdio.h>
class Solution {
public:
    vector<int> findDuplicates(vector<int>& nums) {
        ios::sync_with_stdio(false);
        cin.tie(nullptr);
        cout.tie(nullptr);
        vector<int> v;
        int n = nums.size();
        for(int i=0;i<nums.size();i++){
            int x = abs(nums[i]);
            nums[x-1]*=-1;
            if(nums[x-1]>0){
                v.push_back(x);
            }
        }
        return v;
    }
};
